{
  "name": "solid-start",
  "scripts": {
    "dev": "solid-start dev --host",
    "build": "solid-start build && npm run postbuild",
    "serve": "vite preview",
    "start": "solid-start start",
    "format": "prettier -w src/*",
    "push": "prisma db push",
    "postinstall": "prisma generate",
    "postbuild": "cp node_modules/.pnpm/**/@prisma/engines/*query* .vercel/output/functions/render.func/ && cp prisma/schema.prisma .vercel/output/functions/render.func/"
  },
  "type": "module",
  "devDependencies": {
    "@types/node": "^18.11.17",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "prisma": "^4.8.0",
    "solid-start-node": "^0.2.8",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "vite": "^4.0.3"
  },
  "dependencies": {
    "@auth/core": "^0.1",
    "@prisma/client": "^4.8.0",
    "@solid-auth/core": "^0.0.6",
    "@solid-auth/next": "^0.0.19",
    "@solid-auth/socials": "^0.0.8",
    "@solidjs/meta": "^0.28.2",
    "@solidjs/router": "^0.6.0",
    "@trpc/client": "^10.7.0",
    "@trpc/server": "^10.7.0",
    "dotenv": "^16.0.3",
    "solid-js": "^1.6.6",
    "solid-start": "^0.2.8",
    "solid-start-trpc": "^0.0.16",
    "solid-start-vercel": "^0.2.8",
    "undici": "^5.14.0",
    "zod": "^3.20.2"
  },
  "engines": {
    "node": ">=16"
  }
}
