{
  "name": "solid-start",
  "scripts": {
    "dev": "solid-start dev --host",
    "build": "solid-start build && npm run postbuild",
    "serve": "vite preview",
    "start": "solid-start start",
    "format": "prettier -w src/*",
    "push": "prisma db push",
    "postinstall": "prisma generate",
    "postbuild": "cp node_modules/.pnpm/**/@prisma/engines/*query* .vercel/output/functions/render.func/ && cp prisma/schema.prisma .vercel/output/functions/render.func/"
  },
  "type": "module",
  "devDependencies": {
    "@types/node": "^18.11.17",
    "autoprefixer": "^10.4.12",
    "postcss": "^8.4.18",
    "prettier": "^2.8.1",
    "prisma": "^4.7.1",
    "solid-start-node": "^0.2.0",
    "tailwindcss": "^3.2.0",
    "typescript": "^4.8.4",
    "vite": "^4.0.0"
  },
  "dependencies": {
    "@auth/core": "^0.1.4",
    "@prisma/client": "^4.7.1",
    "@solid-auth/core": "^0.0.6",
    "@solid-auth/next": "^0.0.20",
    "@solid-auth/socials": "^0.0.8",
    "@solidjs/meta": "^0.28.0",
    "@solidjs/router": "^0.5.0",
    "dotenv": "^16.0.3",
    "solid-js": "^1.6.2",
    "solid-start": "^0.2.0",
    "solid-start-vercel": "^0.2.7",
    "undici": "^5.11.0",
    "zod": "^3.20.2"
  },
  "engines": {
    "node": ">=16"
  }
}
